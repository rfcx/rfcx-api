{
  "name": "rfcx-api",
  "version": "1.3.13",
  "license": "UNLICENSED",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/rfcx/rfcx-api.git"
  },
  "scripts": {
    "dev": "yarn dev:core",
    "dev:core": "nodemon core/main.js",
    "dev:noncore": "nodemon noncore/main.js",
    "dev:tasks": "nodemon tasks/main.js",
    "serve-db:core": "docker compose up -d --wait && docker compose exec -T -e PGPASSWORD=test timescaledb pg_isready && yarn rms:core",
    "serve-db:noncore": "docker compose up -d --wait && docker compose exec -T -e PGPASSWORD=test timescaledb pg_isready && yarn rms:noncore",
    "serve-db:test": "docker compose -f test/docker-compose.yaml up -d --quiet-pull",
    "start": "yarn start:core",
    "start:core": "node core/main.js",
    "start:noncore": "node noncore/main.js",
    "start:mqtt": "node mqtt/main.js",
    "start:tasks": "node tasks/main.js",
    "reset:core": "node common/_cli/reset.js core",
    "reset:noncore": "node common/_cli/reset.js noncore",
    "migrate:core": "node common/_cli/migrate.js core",
    "migrate:noncore": "node common/_cli/migrate.js noncore",
    "seed:core": "bash ./common/_cli/seed.sh -d core -f core",
    "seed:noncore": "bash ./common/_cli/seed.sh -d noncore -f noncore",
    "rms:core": "yarn reset:core && yarn migrate:core && yarn seed:core",
    "rms:noncore": "yarn reset:noncore && yarn migrate:noncore && yarn seed:noncore",
    "lint": "eslint \"**/*.js\"",
    "lint-fix": "eslint --fix \"**/*.js\"",
    "test:unit": "jest --forceExit --config ./test/jest.unit.config.js",
    "test:int": "jest --runInBand --forceExit --config ./test/jest.int.config.js",
    "test": "yarn test:unit && yarn test:int"
  },
  "dependencies": {
    "@google-cloud/storage": "7.7.0",
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "@rapideditor/country-coder": "^5.2.2",
    "@rfcx/message-queue": "^1.0.8",
    "@xmldom/xmldom": "0.8.10",
    "archiver": "6.0.1",
    "ascii85": "1.0.2",
    "aws-sdk": "2.920.0",
    "bluebird": "3.7.2",
    "body-parser": "1.20.2",
    "cors": "2.8.5",
    "country-reverse-geocoding": "^0.2.2",
    "csprng": "0.1.2",
    "express": "4.18.2",
    "express-prom-bundle": "^6.0.0",
    "express-request-id": "1.4.1",
    "express-winston": "4.2.0",
    "firebase-admin": "12.0.0",
    "fluent-ffmpeg": "2.1.2",
    "generate-password": "1.7.1",
    "geo-tz": "^8.0.1",
    "handlebars": "^4.7.7",
    "jimp": "0.22.10",
    "jwks-rsa-passport-edition": "1.2.2",
    "knox-s3": "0.9.5",
    "lodash": "4.17.21",
    "mandrill-api": "1.0.45",
    "moment": "2.30.1",
    "moment-timezone": "0.5.44",
    "mqtt": "5.3.4",
    "multer": "^1.4.5-lts.1",
    "newrelic": "11.7.0",
    "node-cache": "^5.1.2",
    "node-unzip-2": "0.2.8",
    "parallel-download": "1.1.1",
    "passport": "0.7.0",
    "passport-accesstoken": "0.1.0",
    "passport-jwt": "4.0.1",
    "pdfmake": "0.2.9",
    "pg": "^8.7.1",
    "pg-hstore": "2.3.4",
    "probe-image-size": "7.2.3",
    "prom-client": "15.1.0",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "roboto-font": "0.1.0",
    "sequelize": "5.22.5",
    "sha.js": "2.4.11",
    "stripe": "14.10.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0",
    "twilio": "4.20.0",
    "uuid": "9.0.1",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.1",
    "eslint-plugin-n": "16.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "jest": "^29.5.0",
    "nodemon": "3.0.2",
    "s3": "4.4.0",
    "sequelize-cli": "5.5.1",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3"
  }
}
