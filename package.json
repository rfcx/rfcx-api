{
  "name": "rfcx-api",
  "version": "1.3.0",
  "license": "UNLICENSED",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/rfcx/rfcx-api.git"
  },
  "scripts": {
    "dev": "npm run dev:core",
    "dev:core": "nodemon core/main.js",
    "dev:noncore": "nodemon noncore/main.js",
    "dev:tasks": "nodemon tasks/main.js",
    "serve-db:core": "docker compose up -d --wait && docker compose exec -T -e PGPASSWORD=test timescaledb pg_isready && npm run rms:core",
    "serve-db:noncore": "docker compose up -d --wait && docker compose exec -T -e PGPASSWORD=test timescaledb pg_isready && npm run rms:noncore",
    "serve-db:test": "docker compose -f test/docker-compose.yaml up -d --quiet-pull",
    "start": "npm run start:core",
    "start:core": "node core/main.js",
    "start:noncore": "node noncore/main.js",
    "start:mqtt": "node mqtt/main.js",
    "start:tasks": "node tasks/main.js",
    "reset:core": "node common/_cli/reset.js core",
    "reset:noncore": "node common/_cli/reset.js noncore",
    "migrate:core": "node common/_cli/migrate.js core",
    "migrate:noncore": "node common/_cli/migrate.js noncore",
    "seed:core": "bash ./common/_cli/seed.sh -d core -f core",
    "seed:noncore": "bash ./common/_cli/seed.sh -d noncore -f noncore",
    "rms:core": "npm run reset:core && npm run migrate:core && npm run seed:core",
    "rms:noncore": "npm run reset:noncore && npm run migrate:noncore && npm run seed:noncore",
    "lint": "eslint \"**/*.js\"",
    "lint-fix": "eslint --fix \"**/*.js\"",
    "test:unit": "jest --forceExit --config ./test/jest.unit.config.js",
    "test:int": "jest --runInBand --forceExit --config ./test/jest.int.config.js",
    "test": "npm run test:unit && npm run test:int"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.2.0",
    "@rfcx/message-queue": "^1.0.8",
    "@tmcw/togeojson": "^4.5.0",
    "archiver": "4.0.1",
    "ascii85": "1.0.2",
    "aws-sdk": "2.920.0",
    "bluebird": "2.11.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "country-reverse-geocoding": "^0.2.2",
    "csprng": "0.1.2",
    "express": "4.17.1",
    "express-prom-bundle": "^6.0.0",
    "express-request-id": "1.4.1",
    "express-winston": "4.0.5",
    "firebase-admin": "6.1.0",
    "fluent-ffmpeg": "2.1.2",
    "generate-password": "1.4.2",
    "handlebars": "^4.7.7",
    "jimp": "^0.13.0",
    "jwks-rsa-passport-edition": "1.2.2",
    "knox": "0.9.2",
    "mandrill-api": "1.0.45",
    "moment": "2.25.3",
    "moment-timezone": "^0.5.34",
    "mqtt": "^4.2.1",
    "multer": "0.1.8",
    "mysql2": "2.1.0",
    "newrelic": "^8.17.0",
    "node-cache": "^5.1.2",
    "node-unzip-2": "0.2.8",
    "parallel-download": "1.1.0",
    "passport": "0.4.1",
    "passport-accesstoken": "0.1.0",
    "passport-jwt": "4.0.0",
    "pdfmake": "0.1.53",
    "pg": "^8.7.1",
    "pg-hstore": "2.3.3",
    "probe-image-size": "5.0.0",
    "prom-client": "^12.0.0",
    "request": "2.88.2",
    "request-promise": "4.2.1",
    "roboto-font": "0.1.0",
    "sequelize": "5.22.5",
    "sha.js": "2.4.8",
    "sqs-consumer": "^5.5.0",
    "stripe": "6.20.1",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "twilio": "3.54.1",
    "tz-lookup": "^6.1.25",
    "uuid": "^8.3.2",
    "winston": "3.3.3",
    "xmldom": "^0.4.0"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.16",
    "s3": "4.4.0",
    "sequelize-cli": "5.5.1",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3"
  }
}
