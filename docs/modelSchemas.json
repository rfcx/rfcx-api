{
  "AnnotationLite": {
    "allOf": [
      {
        "$ref": "#/components/requestBodies/Annotation"
      },
      {
        "properties": {
          "id": {
            "type": "uuid",
            "example": "c0686679-6c06-4f5d-84b5-2b4934aad33e"
          },
          "stream_id": {
            "type": "string",
            "example": "01a2ucztj8ax"
          },
          "classification": {
            "$ref": "#/components/schemas/ClassificationLite"
          }
        },
        "required": [
          "id",
          "streamId"
        ]
      }
    ]
  },
  "Annotation": {
    "allOf": [
      {
        "$ref": "#/components/schemas/AnnotationLite"
      },
      {
        "properties": {
          "created_at": {
            "type": "date"
          },
          "created_by": {
            "type": "integer",
            "description": "The user who created the annotation"
          },
          "updated_at": {
            "type": "date"
          },
          "updated_by": {
            "type": "integer",
            "description": "The user who last updated the annotation"
          }
        }
      }
    ]
  },
  "ClassificationLite": {
    "type": "object",
    "properties": {
      "value": {
        "type": "string",
        "description": "Unique short name",
        "example": "falciformis"
      },
      "title": {
        "type": "string",
        "description": "Description or latin name",
        "example": "Carcharhinus falciformis"
      },
      "image": {
        "type": "string"
      }
    },
    "required": [
      "value",
      "title"
    ]
  },
  "Classification": {
    "allOf": [
      {
        "$ref": "#/components/schemas/ClassificationLite"
      },
      {
        "properties": {
          "type": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string",
                "example": "species"
              }
            }
          },
          "description": {
            "type": "string",
            "example": "Found in temperate and tropical seas all around the world, they begin their lives spotless and steadily gain spots as they get older."
          },
          "frequency_min": {
            "type": "integer",
            "example": 4500
          },
          "frequency_max": {
            "type": "integer",
            "example": 9800
          },
          "alternative_names": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "language_id": {
                  "type": "string"
                }
              }
            },
            "example": [
              {
                "name": "Bridled Dolphin",
                "language_id": "en_US"
              },
              {
                "name": "Pantropical Spotted Dolphin",
                "language_id": "en_US"
              }
            ]
          },
          "reference_annotations": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "uuid"
                },
                "stream_id": {
                  "type": "string"
                },
                "start": {
                  "type": "date"
                },
                "end": {
                  "type": "date"
                },
                "frequency_min": {
                  "type": "integer"
                },
                "frequency_max": {
                  "type": "integer"
                }
              }
            }
          }
        }
      }
    ]
  },
  "ClassifierLite": {
    "properties": {
      "id": {
        "type": "integer",
        "example": 12
      },
      "name": {
        "type": "string",
        "example": "chainsaw"
      },
      "version": {
        "type": "integer",
        "example": 8
      },
      "status": {
        "type": "string",
        "description": "One of 'draft' (default), 'staging', 'active', 'retired'"
      },
      "external_id": {
        "type": "string",
        "description": "Identifier in AI Hub or other service"
      }
    },
    "required": [
      "id",
      "name",
      "version"
    ]
  },
  "Classifier": {
    "allOf": [
      {
        "$ref": "#/components/schemas/ClassifierLite"
      },
      {
        "properties": {
          "supported_classifications": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ClassificationLite"
            }
          },
          "enabled_projects": {
            "type": "array",
            "description": "Projects that are enabled for this classifier",
            "items": {
              "type": "string"
            }
          },
          "enabled_streams": {
            "type": "array",
            "description": "Individual streams that are enabled for this classifier",
            "items": {
              "$ref": "#/components/schemas/StreamLite"
            }
          },
          "created_at": {
            "type": "date"
          },
          "created_by": {
            "$ref": "#/components/schemas/UserLite"
          },
          "updated_at": {
            "type": "date"
          },
          "updated_by": {
            "$ref": "#/components/schemas/UserLite"
          }
        }
      }
    ]
  },
  "Cluster": {
    "type": "object",
    "properties": {
      "time_bucket": {
        "type": "date",
        "description": "Timestamp of the start of the time bucket",
        "example": "2020-06-02T00:00:00.000Z"
      },
      "aggregated_value": {
        "type": "float",
        "description": "The result of the aggregation over the cluster (sum, count, avg, etc)",
        "example": 72
      },
      "first_start": {
        "type": "date",
        "description": "Timestamp of the first annotation in the cluster",
        "example": "2020-05-12T07:30:21.500Z"
      },
      "last_start": {
        "type": "date",
        "description": "Timestamp of the last annotation in the cluster",
        "example": "2020-05-12T12:48:21.500Z"
      },
      "classification": {
        "$ref": "#/components/schemas/ClassificationLite"
      }
    }
  },
  "AnnotationCluster": {
    "$ref": "#/components/schemas/Cluster"
  },
  "DetectionCluster": {
    "$ref": "#/components/schemas/Cluster"
  },
  "Review": {
    "properties": {
      "positive": {
        "type": "boolean",
        "example": true
      },
      "created_at": {
        "type": "date",
        "example": "2020-05-13T09:25:21.500Z"
      },
      "user": {
        "$ref": "#/components/schemas/UserLite"
      }
    }
  },
  "Detection": {
    "properties": {
      "id": {
        "type": "string",
        "example": "cf8a7ac9-0dd2-40b6-b03f-a067d6c43325"
      },
      "stream_id": {
        "type": "string",
        "example": "xu82jDgX49"
      },
      "start": {
        "type": "date",
        "example": "2020-05-12T07:30:21.500Z"
      },
      "end": {
        "type": "date",
        "example": "2020-05-12T07:30:22.500Z"
      },
      "confidence": {
        "type": "float",
        "example": 0.55471558926584
      },
      "classification": {
        "$ref": "#/components/schemas/ClassificationLite"
      }
    }
  },
  "DetectionWithReviews": {
    "allOf": [
      {
        "$ref": "#/components/schemas/Detection"
      },
      {
        "properties": {
          "reviews": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Review"
            }
          }
        }
      }
    ]
  },
  "StreamLite": {
    "properties": {
      "id": {
        "type": "string",
        "example": "xu82jDgX49"
      },
      "name": {
        "type": "string",
        "example": "My Stream 1"
      },
      "start": {
        "type": "date",
        "example": "2020-05-12T07:30:21.500Z"
      },
      "end": {
        "type": "date",
        "example": "2020-05-12T07:30:22.500Z"
      },
      "is_public": {
        "type": "boolean",
        "example": false
      }
    },
    "required": [
      "name"
    ]
  },
  "Stream": {
    "allOf": [
      {
        "$ref": "#/components/schemas/StreamLite"
      },
      {
        "properties": {
          "description": {
            "type": "string",
            "example": "Stream 1 description"
          },
          "created_at": {
            "type": "date",
            "example": "2020-05-13T07:30:22.500Z"
          },
          "created_by": {
            "$ref": "#/components/schemas/UserLite"
          },
          "updated_at": {
            "type": "date",
            "example": "2020-05-13T07:30:22.500Z"
          },
          "max_sample_rate": {
            "type": "integer",
            "example": 32000
          },
          "latitude": {
            "type": "float",
            "example": 90.123
          },
          "longitude": {
            "type": "float",
            "example": 125.912
          },
          "project": {
            "$ref": "#/components/schemas/ProjectLite"
          }
        },
        "required": [
          "name"
        ]
      }
    ]
  },
  "StreamWithPermissions": {
    "allOf": [
      {
        "$ref": "#/components/schemas/Stream"
      },
      {
        "properties": {
          "permissions": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["O", "W", "R"]
          }
        }
      }
    ]
  },
  "UserLite": {
    "properties": {
      "firstname": {
        "type": "string",
        "example": "John"
      },
      "lastname": {
        "type": "string",
        "example": "Doe"
      },
      "email": {
        "type": "string",
        "example": "john@doe.com"
      },
      "picture": {
        "type": "string",
        "example": "https://upload.wikimedia.org/wikipedia/commons/1/11/Contact-new.svg"
      }
    }
  },
  "UserLiteWithRoleAndPermissions": {
    "allOf": [
      {
        "$ref": "#/components/schemas/UserLite"
      },
      {
        "properties": {
          "role": {
            "type": "string",
            "example": "Member"
          },
          "permissions": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["O", "W", "R"]
          }
        }
      }
    ]
  },
  "IndexLite": {
    "properties": {
      "code": {
        "type": "string",
        "example": "ad"
      },
      "name": {
        "type": "string",
        "example": "Acoustic Diversity Index"
      },
      "range_min": {
        "type": "float",
        "example": 0.0
      },
      "range_max": {
        "type": "float",
        "example": 1.0
      }
    }
  },
  "IndexValueLite": {
    "properties": {
      "time": {
        "type": "date",
        "example": "2020-05-13T07:30:00.000Z"
      },
      "value": {
        "type": "float",
        "example": 0.1234
      }
    }
  },
  "StreamSourceFileLite": {
    "properties": {
      "id": {
        "type": "uuid",
        "example": "c0686679-6c06-4f5d-84b5-2b4934aad33e"
      },
      "filename": {
        "type": "string",
        "example": "1feb70b91439-2020-06-24T17-15-26.opus"
      },
      "duration": {
        "type": "integer",
        "example": 90126
      }
    }
  },
  "StreamSourceFile": {
    "allOf": [
      {
        "$ref": "#/components/schemas/StreamSourceFileLite"
      },
      {
        "properties": {
          "stream": {
            "$ref": "#/components/schemas/StreamLite"
          },
          "audio_file_format": {
            "type": "string",
            "example": "opus"
          },
          "sample_count": {
            "type": "integer",
            "example": 4326072
          },
          "sample_rate": {
            "type": "integer",
            "example": 12000
          },
          "channels_count": {
            "type": "integer",
            "example": 1
          },
          "bit_rate": {
            "type": "integer",
            "example": 16384
          },
          "audio_codec": {
            "type": "string",
            "example": "opus"
          },
          "sha1_checksum": {
            "type": "string",
            "example": "7ea9896798385afb5939cdf8349ce389f5e3b128"
          },
          "meta": {
            "type": "object",
            "example": {
              "date": "2020-06-24T10:11:38-07",
              "title": "SBW1607_20200624_171138.wav",
              "album": "SBx-ETH R4"
            }
          }
        }
      }
    ]
  },
  "StreamSegment": {
    "properties": {
      "id": {
        "type": "uuid",
        "example": "e44e4fb1-df10-4176-9f8c-e989ccf4d0d8"
      },
      "stream": {
        "$ref": "#/components/schemas/StreamLite"
      },
      "start": {
        "type": "date",
        "example": "2020-05-12T07:30:21.500Z"
      },
      "end": {
        "type": "date",
        "example": "2020-05-12T07:30:22.500Z"
      },
      "sample_count": {
        "type": "integer",
        "example": 4326072
      },
      "stream_source_file": {
        "$ref": "#/components/schemas/StreamSourceFileLite"
      },
      "file_extension": {
        "type": "string",
        "example": "opus"
      }
    }
  },
  "OrganizationLite": {
    "properties": {
      "id": {
        "type": "string",
        "example": "128hqwgX49"
      },
      "name": {
        "type": "string",
        "example": "Rainforest Connection"
      },
      "is_public": {
        "type": "boolean",
        "example": false
      }
    }
  },
  "Organization": {
    "allOf": [
      {
        "$ref": "#/components/schemas/OrganizationLite"
      },
      {
        "properties": {
          "created_at": {
            "type": "date",
            "example": "2020-05-13T07:30:22.500Z"
          },
          "created_by": {
            "$ref": "#/components/schemas/UserLite"
          },
          "updated_at": {
            "type": "date",
            "example": "2020-05-13T07:30:22.500Z"
          }
        }
      }
    ]
  },
  "ProjectLite": {
    "properties": {
      "id": {
        "type": "string",
        "example": "xu82jDgX49"
      },
      "name": {
        "type": "string",
        "example": "My Project 1"
      },
      "is_public": {
        "type": "boolean",
        "example": false
      }
    },
    "required": [
      "name"
    ]
  },
  "Project": {
    "allOf": [
      {
        "$ref": "#/components/schemas/ProjectLite"
      },
      {
        "properties": {
          "description": {
            "type": "string",
            "example": "Project 1 description"
          },
          "created_at": {
            "type": "date",
            "example": "2020-05-13T07:30:22.500Z"
          },
          "created_by": {
            "$ref": "#/components/schemas/UserLite"
          },
          "updated_at": {
            "type": "date",
            "example": "2020-05-13T07:30:22.500Z"
          },
          "organization": {
            "$ref": "#/components/schemas/OrganizationLite"
          }
        },
        "required": [
          "name"
        ]
      }
    ]
  },
  "ProjectPublic": {
    "properties": {
      "name": {
        "type": "string",
        "example": "My Project 1"
      },
      "description": {
        "type": "string",
        "example": "Project 1 description"
      }
    }
  },
  "ProjectPublicWithLocation": {
    "allOf": [
      {
        "$ref": "#/components/schemas/ProjectPublic"
      },
      {
        "properties": {
          "latitude": {
            "type": "float",
            "example": 90.123
          },
          "longitude": {
            "type": "float",
            "example": 125.912
          }
        }
      }
    ]
  },
  "Permission": {
    "properties": {
      "stream": {
        "$ref": "#/components/schemas/StreamLite"
      },
      "created_by": {
        "$ref": "#/components/schemas/UserLite"
      },
      "organization": {
        "$ref": "#/components/schemas/OrganizationLite"
      },
      "type": {
        "type": "string",
        "example": "R"
      },
      "created_at": {
        "type": "date",
        "example": "2020-05-13T07:30:22.500Z"
      },
      "updated_at": {
        "type": "date",
        "example": "2020-05-13T07:30:22.500Z"
      }
    }
  },
  "UploadsStatistics": {
    "properties": {
      "uploads": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "streamName": {
              "type": "string"
            },
            "recentUploads": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "month": {
                    "type": "string"
                  },
                  "value": {
                    "type": "number"
                  }
                }
              }
            }
          }
        },
        "example": [
          {
              "streamName": "City bells",
              "recentUploads": [
                  {
                      "month": "May",
                      "value": 1
                  },
                  {
                      "month": "Jun",
                      "value": 2
                  },
                  {
                      "month": "Jul",
                      "value": 0
                  }
              ]
          }
        ]
      },
      "totalUploads": {
        "type": "number",
        "example": 3
      },
      "totalHours": {
        "type": "float",
        "example": 0.15913981481481482
      }
    }
  },
  "AnnotationsStatistics": {
    "properties": {
      "frequentClassifications": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "value": {
              "type": "number"
            }
          }
        },
        "example": [
          {
            "name": "Accipiter butleri",
            "value": 5
          },
          {
              "name": "Aonyx capensis",
              "value": 4
          },
          {
              "name": "Orcinus orca",
              "value": 3
          },
          {
              "name": "Bunopithecus hoolock",
              "value": 3
          }
        ]
      },
      "totalAnnotations": {
        "type": "number",
        "example": 18
      },
      "totalClassifications": {
        "type": "number",
        "example": 6
      },
      "weeklyIncreasedPercentage": {
        "type": "number",
        "example": 300
      }
    }
  },
  "DetectionsStatistics": {
    "properties": {
      "totalDetections": {
        "type": "number",
        "example": 300
      },
      "totalVerified": {
        "type": "number",
        "totalVerified": 20
      }
    }
  },
  "EventStrategy": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "Unique identifier for event strategies",
        "example": 15
      },
      "name": {
        "type": "string",
        "description": "Human-readable name of the strategy",
        "example": "Min 4 detections over 90 seconds"
      },
      "description": {
        "type": "string",
        "description": "Description of the strategy",
        "example": "Raise an event when 4 or more detections are found within a 90 second period."
      },
      "type": {
        "type": "string",
        "description": "Type of strategy (one of 'window' or ...)",
        "example": "window"
      },
      "parameters": {
        "type": "string",
        "description": "Parameters to be applied to algorithm",
        "example": "minimum-windows=4 period-seconds=90"
      }
    }
  },
  "EventLite": {
    "properties": {
      "id": {
        "type": "string",
        "example": "cf8a7ac9-0dd2-40b6-b03f-a067d6c43325"
      },
      "stream_id": {
        "type": "string",
        "example": "xu82jDgX49"
      },
      "start": {
        "type": "date",
        "example": "2020-05-12T07:30:21.500Z"
      },
      "end": {
        "type": "date",
        "example": "2020-05-12T07:30:22.500Z"
      },
      "classification": {
        "$ref": "#/components/schemas/ClassificationLite"
      }
    }
  },
  "Event": {
    "allOf": [
      {
        "$ref": "#/components/schemas/EventLite"
      },
      {
        "properties": {
          "stream": {
            "$ref": "#/components/schemas/StreamLite"
          },
          "classifier_event_strategy": {
            "type": "object",
            "properties": {
              "parameters": {
                "type": "string",
                "example": "min-gap-between-events=30m"
              },
              "event_strategy": {
                "$ref": "#/components/schemas/EventStrategy"
              },
              "classifier": {
                "$ref": "#/components/schemas/ClassifierLite"
              }
            }
          }
        }
      }
    ]
  }
}
